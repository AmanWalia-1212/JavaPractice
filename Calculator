public class Calculator {
    static int add(int x, int y){
        int z = x + y;
        return z;
    }
    
     static int substract(int x, int y){
        int z = x - y;
        return z;
    }
    
    static int multiply(int x, int y){
        int z = x * y;
        return z;
    }
    
    static int divide(int x, int y){
        int z = x / y;
        return z;
    }
   
    public static void main(String[] args)
    {
        
        int num1 = 10, num2 = 5;
        int sum, sub, multi, div;
        sum = add(num1, num2);
        System.out.println("Sum of given number is " + sum);
        sub = substract(num1, num2);
        System.out.println("Substraction of given number is " + sub);
        multi = multiply(num1, num2);
        System.out.println("Multiplication of given number is " + multi);
        div = divide(num1, num2);
        System.out.println("Division of given number is " + div);
        
    }
}
