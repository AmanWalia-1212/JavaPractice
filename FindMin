
public class FindMin{

     public static void main(String []args){
       
       int A[] = {1,2,3,4,5};
       int min=A[0];
       
       for (int i=0;i<A.length;i++)
       {
          if(min > A[i]) {
              min = A[i];
          }
       }
       System.out.println("Min No is :" + min);
       }
}
