public class PrintEvenElement {  
    public static void main(String[] args) {  
 
        int [] A = new int [] {1, 2, 3, 4, 5};  
        System.out.println("Elements of array present on even position: ");  
        for (int i = 1; i < A.length; i = i+2) {  
            System.out.println(A[i]);  
        }  
    }  
}
